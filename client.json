{
    "name":"NmpReporting",
    "apiVersion":"1.0",
    "base_url":"https://p4apic.emv2.com/apinmpreporting",
    "description":"Campaign Commander NMP Reporting API",
    "operations":{
        "OpenConnection":{
            "httpMethod":"POST",
            "uri":"connection",
            "summary":"This method provides a session token when given valid credentials.",
            "data":{
                "xmlRoot":{
                    "name":"connection"
                }
            },
            "parameters":{
                "login":{
                    "type":"string",
                    "location":"xml",
                    "required":true
                },
                "pwd":{
                    "type":"string",
                    "location":"xml",
                    "required":true
                },
                "key":{
                    "type":"string",
                    "location":"xml",
                    "required":true
                }
            }
        },
        "CloseConnection":{
            "httpMethod":"DELETE",
            "uri":"connection",
            "summary":"This method terminate the session token",
            "responseClass":"primitive",
            "parameters":{
                "token":{
                    "location":"header",
                    "type":"string",
                    "required":true
                }
            }
        },
        "BounceReport":{
            "httpMethod":"GET",
            "uri":"report/bounce",
            "summary":"This method is used to generate bounce reports in xml and json formats.",
            "responseClass":"ListBounceReport",
            "parameters":{
                "token":{
                    "location":"header",
                    "type":"string",
                    "required":true
                }
            }
        },

        "model":{
            "ListBounceReport": {
                "type": "array",
                "items": {
                    "type": "object",
                    "parentClass": "BounceReport"
                }
            },
            "BounceReport":{
                "type":"object",
                "properties":{
                    "name":{
                        "type":"string",
                        "location":"json"
                    },
                    "nmMessageId":{
                        "type":"number",
                        "location":"json"
                    },
                    "memberUID":{
                        "type":"number",
                        "location":"json"
                    },
                    "email":{
                        "type":"string",
                        "location":"json"
                    },
                    "requestDate":{
                        "type":"string",
                        "location":"json"
                    },
                    "processDate":{
                        "type":"string",
                        "location":"json"
                    },
                    "sendDate":{
                        "type":"string",
                        "location":"json"
                    },
                    "smptErrorCode":{
                        "type":"number",
                        "location":"json"
                    },
                    "bounceType":{
                        "type":"number",
                        "location":"json"
                    }
                }
            }
        }
    }
}